<configuration>
   <system.webServer>
      <staticContent>
         <mimeMap fileExtension=".json" mimeType="application/json" />
         <remove fileExtension=".woff"/>
         <mimeMap fileExtension=".woff" mimeType="application/font-woff" />
         <mimeMap fileExtension=".woff2" mimeType="font/woff2" />
   </staticContent>
<rewrite>
        <rules>
            <rule name="Angular Routes fr" stopProcessing="true">
                <match url="(.*)" />
                <conditions logicalGrouping="MatchAll">
                     <add input="{HTTP_HOST}" pattern="https://hisham-test-serveur.azurewebsites.net" /> 
                   
                </conditions>
                <action type="Redirect" url="https://hisham-test-serveur.azurewebsites.net/jp" />
            </rule>
           
        </rules>
    </rewrite>
   </system.webServer>
</configuration>